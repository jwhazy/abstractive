{
  "build": {
    "beforeDevCommand": "yarn vite:dev",
    "beforeBuildCommand": "yarn vite:build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Abstractive",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "dev.jacksta.abstractive",
      "longDescription": "In-development Fortnite mod marketplace.",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "In-development Fortnite mod marketplace.",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 660,
        "resizable": true,
        "title": "Abstractive",
        "width": 1240,
        "decorations": false,
        "transparent": true,
        "visible": false
      },
      {
        "width": 1240,
        "height": 660,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "resizable": false,
        "fullscreen": false,
        "title": "Abstractive is loading..."
      }
    ]
  }
}
